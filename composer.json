{
  "name": "markheydon/lottery-generator",
  "description": "Just for fun, makes an attempt at 'guessing the Lotto numbers using a half-arsed bit of logic.",
  "version": "1.0.0",
  "keywords": [
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Mark Heydon",
      "email": "contact@mhcg.co.uk",
      "role": "Developer"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=7.1",
    "zfcampus/zf-console": "^1.3",
    "zendframework/zend-filter": "^2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|6.5.*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "phpcompatibility/php-compatibility": "*"
  },
  "autoload": {
    "psr-4": {
      "MarkHeydon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MarkHeydonTests\\": "tests/"
    }
  },
  "bin": [
    "bin/cli.php"
  ],
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ],
    "post-install-cmd": [
      "@install-codestandards"
    ]
  }
}
